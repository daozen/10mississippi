<template>
    <view id="article-page">
        <view class="header" @longpress="headerLongPressEvent">
            <view class="info">
                <image src="../../static/img/lamp.png"></image>
                <view class="title">
                    文字
                    <view>观察</view>
                </view>
            </view>
        </view>
        <scroll-view scroll-x class="category">
            <view class="item">专辑</view>
            <repeat for="{{category}}">
                <view class="item" @tap="goToCategory({{item._id}})">{{item.title}}</view>
            </repeat>
        </scroll-view>
        <view class="list">
            <repeat for="{{articleList}}">
                <view class="item" @longpress="showDescription({{item.description}})" @tap="goToMovieDetail({{item._id}})">
                    <image src="{{item.cover}}" mode="aspectFill" lazy-load="true" @error="picLoadError" class="cover"></image>
                    <view class="info">
                        <view class="title">{{item.title}}</view>
                        <view class="details">
                            <view class="author">
                                <image src="../../static/img/author.png"></image>
                                {{item.author}}
                            </view>
                        </view>
                        <view style="clear: both"></view>
                    </view>
                    <view class="repo" wx:if="{{item.isRepo}}">转载</view>
                </view>
            </repeat>
        </view>
    </view>
</template>

<script>
    import wepy from 'wepy'
    import ls from '../../utils/localstorage'
    import * as http from '../../utils/http'

    export default class Article extends wepy.page {
        config = {
            navigationBarTitleText: '文字',
            navigationBarBackgroundColor: '#ffb11b',
            enablePullDownRefresh: true,
            backgroundColor: '#a96a03'
        }

        data = {
            category: [],
            articleList: []
        }

        methods = {
            headerLongPressEvent() {
                wepy.showModal({
                    title: 'Suprise!',
                    content: '很好，你很闲。',
                    showCancel: false
                })
            },
            showDescription(des) {
                wepy.showModal({
                    title: '文章预览',
                    content: des,
                    showCancel: false
                })
            },
            picLoadError(event) {
                console.log(event.detail.errMsg)
            },
            goToCategory(id) {
                wepy.showToast({
                    title: '分类功能尚未完善。',
                    icon: 'none'
                })
            },
            goToMovieDetail(id) {
                wepy.navigateTo({
                    url: 'articledetail?id=' + id
                })
            }
        }

        // 下拉刷新事件监听
        async onPullDownRefresh() {
            await this.getArticleListDirect();
            wx.stopPullDownRefresh();
        }

        // 直接发起数据请求，无需确认是否有缓存
        getArticleListDirect() {
            return new Promise((resolve) => {
                http.GET('/api/mp/category', {
                    page: 1,
                    limit: 12,
                    select: 'title'
                }).then((data) => {
                    this.category = data.docs;
                    this.$apply();
                    ls.Set('articleCategory', this.category);
                })
                http.GET('/api/mp/article', {
                    page: 1,
                    limit: 20,
                    select: 'title description author cover isRepo'
                }).then((data) => {
                    this.articleList = data.docs;
                    this.$apply();
                    ls.Set('articleList', this.articleList);
                    ls.Set('articleListUpdateTime', new Date().getTime());
                })
                resolve();
            });
        }

        // 请求文章分类数据
        getArticleCategory() {
            // 返回一个Promise
            return new Promise((resolve) => {
                // 获取本地缓存
                let category = ls.Get('articleCategory');

                if (category) {
                    this.category = category;
                } else {
                    http.GET('/api/mp/category', {
                        page: 1,
                        limit: 12,
                        select: 'title'
                    }).then((data) => {
                        this.category = data.docs;
                        this.$apply();
                        ls.Set('articleCategory', this.category);
                    })
                }
                resolve();
            });
        }

        // 请求文章列表数据，确认是否有缓存
        getArticleList() {
            return new Promise((resolve) => {
                let articleList = ls.Get('articleList');
                let articleListUpdateTime = ls.Get('articleListUpdateTime');
                let currentTime = new Date().getTime();

                // 检测是否存在本地缓存
                if (articleList) {
                    if (((currentTime - articleListUpdateTime) / 1000 / 60 / 60) >= 2) {
                        http.GET('/api/mp/article', {
                            page: 1,
                            limit: 20,
                            select: 'title description author cover isRepo'
                        }).then((data) => {
                            this.articleList = data.docs;
                            this.$apply();
                            ls.Set('articleList', this.articleList);
                            ls.Set('articleListUpdateTime', new Date().getTime());
                        })
                    } else {
                        this.articleList = articleList;
                    }
                } else {
                    http.GET('/api/mp/article', {
                        page: 1,
                        limit: 20,
                        select: 'title description author cover isRepo'
                    }).then((data) => {
                        this.articleList = data.docs;
                        this.$apply();
                        ls.Set('articleList', this.articleList);
                        ls.Set('articleListUpdateTime', new Date().getTime());
                    })
                }
                return resolve();
            });
        }

        async onLoad() {
            wepy.showLoading({
                title: '数据加载中...',
                mask: true
            });

            // 获取文章分类
            await this.getArticleCategory();

            // 获取文章列表
            await this.getArticleList();

            wepy.hideLoading();
        }
    }
</script>

